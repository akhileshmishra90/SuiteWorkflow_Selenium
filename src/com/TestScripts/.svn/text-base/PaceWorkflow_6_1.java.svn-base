				//Click on QuoteLink of the Order
				DSF.ClickOn_QuoteLinkBasedOnOrderNumber(orderNum);

				//Swiching to Quote Editior Window Page
				DSF.switchTo_RespectivePage(" 	Quote Editor ");

				//Enter the Price For the First Product
				DSF.enter_Price_for_FirstProduct("700");
				DSF.enter_Price_For_SecondPricetext("300");

				//Click on Refresh Button
				DSF.clickOn_refrfeshButton();

				//Click On Save Changes Button
				DSF.click_on_SaveChangesButton();

				String actualSubTotal=DSF.fetchPrice("Subtotal:");
				String actualShipping=DSF.fetchPrice("Shipping:");
				String actualHandling=DSF.fetchPrice("Handling:");
				String actualTaxes=DSF.fetchPrice("Taxes:");
				String actualTotal=DSF.fetchPrice("Total:");

				String ExpSubTotal=dbConnection.ReadFunction(sSheetName, sScenario, sTestcase, "SubTotal");
				String ExpShipping=dbConnection.ReadFunction(sSheetName, sScenario, sTestcase, "Shipping");
				String ExpHandling=dbConnection.ReadFunction(sSheetName, sScenario, sTestcase, "Handling");
				String ExpTaxes=dbConnection.ReadFunction(sSheetName, sScenario, sTestcase, "Taxes");
				String ExpTotal=dbConnection.ReadFunction(sSheetName, sScenario, sTestcase, "Total");

				boolean subTotal=DSF.Compare_Pricing(ExpSubTotal, actualSubTotal);
				boolean Shipping=DSF.Compare_Pricing(ExpShipping, actualShipping);
				boolean Handling=DSF.Compare_Pricing(ExpHandling, actualHandling);
				boolean Taxes=DSF.Compare_Pricing(ExpTaxes, actualTaxes);
				boolean Toatl=DSF.Compare_Pricing(ExpTotal,actualTotal);

				if(subTotal && Shipping && Handling && Taxes &&  Toatl)
				{
					System.out.println("Edited Prices calulated and Updated Correctly In DSf");
				}
				else
				{
					System.err.println("Edited prices either not updated or calculated properly.. Please check the logs");
					System.err.println("Expected Sub Total is :"+ExpSubTotal+ " Actual Subtotal is :"+actualSubTotal+ 
							"\nExpected Shipping is :"+ExpShipping+" Actual Shipping is : " +actualShipping+
							"\nExpected Handling is :"+ExpHandling+" Actual Handling is :" +actualHandling+
							"\nExpected Taxes is :"+ExpTaxes+" Actual Taxes is :" +actualTaxes+
							"\n Expected Total is :"+ExpTotal+" Actual Total is :" +actualTotal );
				}

				dbConnection.UpdateFunction(sSheetName, sScenario, sTestcase, "DSFSubTotal",actualSubTotal);
				dbConnection.UpdateFunction(sSheetName, sScenario, sTestcase, "DSFShippingAmount",actualShipping);
				dbConnection.UpdateFunction(sSheetName, sScenario, sTestcase, "DSFTotalAmt",actualTotal);
				dbConnection.UpdateFunction(sSheetName, sScenario, sTestcase, "DSFTaxAmount",actualTaxes);
				dbConnection.UpdateFunction(sSheetName, sScenario, sTestcase, "DSFHandlingAmount",actualHandling);

				//========================================== verify modified Job Billing prices in Pace =============================================================								
				_register.launchapplication(PaceURL);
				_driver.manage().window().maximize();
				_driver.manage().timeouts().implicitlyWait(20,TimeUnit.SECONDS);
				//_driver = eyes.open(_driver, "PACE", "TC_6_1", new RectangleSize(1008, 705));

				JCCPage JCC = new JCCPage(_driver);
				
				System.out.println("Loging in to Pace");
